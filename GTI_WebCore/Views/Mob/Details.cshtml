@model GTI_WebCore.ViewModels.EmpresaDetailsViewModel

@{ 
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Detalhe de uma empresa";
}

    <div>
        <h2>Dados Cadastrais de uma Empresa</h2>
    </div>
    <br />
    <br />



    <form asp-controller="Mob" asp-action="Details" method="post" class="mt-3">
        <div class="form-group row">
            <label asp-for="Inscricao" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Inscricao" class="form-control" placeholder="Inscrição Municipal">
            </div>
        </div>

        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-primary">Create</button>
            </div>
        </div>
    </form>




